510aDC*                    CALL 'BFCADR'
510aDC*                    PARM           $RTN    8
510aDC*          $RTN      IFEQ '*EXPIRED'
510aDC*                    MOVE 'EOJ'     NXTSCR
510aDC*                    ELSE
     C                     EXSR ZZINZ2
     C           ERROR     IFEQ *ON
     C                     MOVE 'EOJ'     NXTSCR
     C                     END
510aDC*                    END
     C*
     C           NXTSCR    DOWNE'EOJ'
     C                     EXSR DSPSCR
     C           NXTSCR    CASEQ'01 '     SC1
     C                     END
     C                     END
