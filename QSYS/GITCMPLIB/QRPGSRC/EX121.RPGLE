     *----------------------------------------------------------------
     *
     *  ZZFIL1   Fill screen 1 fields with info from file.
     *
     C     zzfil1        begsr
     *
     C                   exsr      zzgeti
      * Get and Load receiving catch weight override information
      /free
         chain ($pwhse:$ptrn#:$pitem) pocwexcp1;
         if %found(pocwexcp1);
           w1inwgt = exwgt;
           w1exswgt= exswgt;
           w1rngl  = exrngl;
           w1rngh  = exrngh;
           w1rg1c  = exrg1c;
           w1rg2c  = exrg2c;
           select;
             when exrg1w = 'I';
               w1rg1w = 'Item';
             when exrg1w = 'S';
               w1rg1w = 'System';
             when exrg1w = 'P';
               w1rg1w = 'PO';
             other;
               w1rg1w = ' ';
           endsl;
           select;
             when exrg2w = 'I';
               w1rg2w = 'Item';
             when exrg2w = 'S';
               w1rg2w = 'System';
             when exrg2w = 'P';
               w1rg2w = 'PO';
             other;
               w1rg2w = ' ';
           endsl;
         else;
           w1inwgt = 0;
           w1exswgt= 0;
           w1rngl  = 0;
           w1rngh  = 0;
           w1rg1c  = 0;
           w1rg2c  = 0;
           w1rg1w  = ' ';
           w1rg2w  = ' ';
         endif;
      /end-free

