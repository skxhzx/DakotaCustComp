     C                   eval      wkokyu = ukey(x)
     C     wkounq        chain     workopt                            78
     C                   if        not *in78
     C                   move      wkoopt        option
     C                   eval      *in21 = wkoerr
     C                   eval      *in20 = wkoerr
     C                   else
     C                   eval      option = *blanks
     C                   eval      *in21 = *off
     C                   eval      *in20 = *off
     C                   endif
     C                   update    dspsfl
     C                   endif
     C                   enddo
     C                   endsr
     *----------------------------------------------------------------
     *
     *  ZZFGTN   Get next record
     *
     C     zzfgtn        begsr
     C     try1          tag
     *
     *    Code to use when we are not using a partial key.
     *
     *R                   READ RECORD                   79
     *
     *    Code to use when we do have a partial key to use.
     *
      /free
        Select;
          when $lwhdp <> ' ';
            reade ($lwhse: $lwhdp) record;
          when $lwhse <> 0;
            reade ($lwhse) record;
        endsl;
        if %eof(licactive9);
          *in79 = *on;
        else;
          *in79 = *off;
          // exclude *SU - start up licenses
          if %Subst(lalcns:1:3) = '*SU';
            candidatefound = *off;
            leavesr;
          endif;
730aM     // exclude RCV,RWO,STG,RTN & USR slots
          if %Subst(ladisp:1:3) = 'RCV' or
             %Subst(ladisp:1:3) = 'STG' or
             %Subst(ladisp:1:3) = 'RTN' or
730aA        %Subst(ladisp:1:3) = 'RWO' or
             %Subst(ladisp:1:3) = 'USR';
            candidatefound = *off;
            leavesr;
          endif;
          candidatefound = *off;
          // does the orginal license exist in LICINFO

          exsr clr$licinfo;

          $lnwhse  = lawhse;
          $lnlcns  = laolcns;
          $lnseq  = 1;

          $dricommand = '*LICINFO';
          $drisubcmd  = '%GET';
          $drisys2upd = 'D';
          exsr zzzdricop;
          if error;
            // this is a candidate to be assigned or activated
            candidatefound = *on;
          else;
            candidatefound = *off;
          endif;
        endif;
      /end-free
     *
     C                   endsr
     *----------------------------------------------------------------
     *
     *  ZZFGTP   Get previous record
     *
     C     zzfgtp        begsr
     C     try2          tag
     *
     *    Code to use when we are not using a partial key.
     *
     *R                   READPRECORD                   79
     *
     *    Code to use when we do have a partial key to use.
     *
      /free
        Select;
          when $lwhdp <> ' ';
            readpe ($lwhse: $lwhdp) record;
          when $lwhse <> 0;
            readpe ($lwhse) record;
        endsl;
        if %eof(licactive9);
          *in79 = *on;
        else;
          *in79 = *off;
          // is this a system license
          if %Subst(lalcns:1:1) = '*';
            // does the orginal license exist in LICINFO

            exsr clr$licinfo;

            $lnwhse  = lawhse;
            $lnlcns  = laolcns;
            $lnseq  = 1;

            $dricommand = '*LICINFO';
            $drisubcmd  = '%GET';
            $drisys2upd = 'D';
            exsr zzzdricop;
            if error;
              // this is a candidate to be assigned or activated
              candidatefound = *on;
            else;
              candidatefound = *off;
            endif;
          else;
            candidatefound = *off;
          endif;
        endif;
      /end-free
