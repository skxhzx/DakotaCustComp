     *----------------------------------------------------------------
     *
     *  ZZUPD3   Update screen 3
     *
     C     zzupd3        begsr
      /free
        select;
          // Activate
          when wkoopt = ' 1';
            // does the new license exist in licactive
            exsr clr$license;

            $liToLcnsU = *on;
            $liToLcns = w3lcnsnew;

            $dricommand = '*LICENSE';
            $drisubcmd  = '%GETLCNS';
            $drisys2upd = 'D';
            exsr zzzdricop;
            if not error;
              // if so - set the laolcns to the new license, update the
              // entered and expiration dates to the new license date fields
              // call drislot with setolcns, send the new license in
              // in the original license field.
              // stole this code from lt312 setfifo logic
              exsr clr$slot;

              $slwhseu = *on;
              $slwhse  = frm$license.$liToWhse;
              $slwhdpu = *on;
              $slwhdp  = frm$license.$liToWhdp;
              $sldispu = *on;
              $sldisp  = frm$license.$liToDisp;
              $slitemu = *on;
              $slitem  = frm$license.$liToItem;
              $slexpdu = *on;
              $slexpd  = $liToExpD;
              $slentdu = *on;
              $slentd  = $liToEntD;
              $slprtyu = *on;
              $slprty  = $liToPrty;
              $saToLcnsu = *on;
              $saToLcns  = w3lcnsold;
              $saActionu = *on;
              $saAction  = 'FX1';
              $saOLcnsu  = *on;
              $saOLcns   = w3lcnsnew;
              $saEmp#u   = *on;
              $saEmp#    = e$emp#;
              $satrn#u = *on;
              $satrn#  = 0;

              $dricommand = '*SLOT   ';
              $drisubcmd  = '%SETOLCNS';
              $drisys2upd = 'D';
              chk4err = *on;
              zmflag  = *on;
              exsr zzzdricop;

              if error;
              endif;

            else;
              // this needs to have dates based on licinfo
              exsr clr$slot;

              $slwhseu = *on;
              $slwhse  = frm$license.$liToWhse;
              $slwhdpu = *on;
              $slwhdp  = frm$license.$liToWhdp;
              $sldispu = *on;
              $sldisp  = frm$license.$liToDisp;
              $slitemu = *on;
              $slitem  = frm$license.$liToItem;
              $slexpdu = *on;
              $slexpd  = $lnexpdate;
              $slentdu = *on;
              $slentd = %dec(%date($lnaddts):*iso);
              //$slentd  = $liToEntD;
              $slprtyu = *on;
              $slprty  = frm$license.$liToPrty;
              $saToLcnsu = *on;
              $saToLcns  = w3lcnsold;
              $saActionu = *on;
              $saAction  = 'FX2';
              $saOLcnsu  = *on;
              $saOLcns   = w3lcnsnew;
              $saEmp#u   = *on;
              $saEmp#    = e$emp#;
              $satrn#u = *on;
              $satrn#  = 0;

              $saFrmLcnsU  = *on;
              $saFrmLcns = w3lcnsold;

              $dricommand = '*SLOT   ';
              $drisubcmd  = '%SETOLCNS';
              $drisys2upd = 'D';
              chk4err = *on;
              zmflag  = *on;
              exsr zzzdricop;

              if error;
              endif;
            endif;
          when wkoopt = ' 2';
            // Assign
        endsl;
      /end-free
     C                   eval      error = *off
     c                   eval      $ldisp = w1disp
     C                   exsr      scr02i
     *
     C     endupd3       endsr

