*  Dairyland       New Warehouse Setup commands.
*                  Update Set cmd to New Whse Nbr.
*  Run this cmd while signed into new whse:
*      RUNSQLSTM SRCFILE(PIRM700/QSQLSESS)
*                SRCMBR(DRYWHSE)
*                COMMIT(*NONE)
*                PROCESS(*RUN)
*                OUTPUT(*ERROR)
*
update crew      set crwhse =525 where crwhse =040;
update docdoor   set ddwhse =525 where ddwhse =040;
update nexttmpl  set tnwhse =525 where tnwhse =040;
update optiond   set opdwhs =525 where opdwhs =040;
update optione   set opewhs =525 where opewhs =040;
update optionz   set opzwhs =525 where opzwhs =040;
update options   set opwhse =525 where opwhse =040;
update pirord#   set nxowhs =525 where nxowhs =040;
update pirrecid# set nriwhse=525 where nriwhse=040;
update pirrte#   set nxrwhs =525 where nxrwhs =040;
update piruser   set uswhse =525 where uswhse =040;
update piruser   set uswhse =525 where uswhse =040;
update shift     set shwhse =525 where shwhse =040;
update slotdef   set sdwhse =525 where sdwhse =040;
update taskdet   set tdwhse =525 where tdwhse =040;
update trkcomp   set tpwhse =525 where tpwhse =040;
update truckd    set tdwhse =525 where tdwhse =040;
update truckh    set thwhse =525 where thwhse =040;
update zoneh     set zhwhse =525 where zhwhse =040;
update zoned     set zdwhse =525 where zdwhse =040;
update warehs    set whcode =525 where whcode =040;
update warehsad  set whacod =525 where whacod =040;
update whdept    set wdwhse =525 where wdwhse =040;
