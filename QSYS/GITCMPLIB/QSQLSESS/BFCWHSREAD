*  Reinhart Foods. New Warehouse Setup commands.
*                  Update Set cmd to New Whse Nbr.
*  Run this cmd while signed into new whse:
*      RUNSQLSTM SRCFILE(PIRMODS/QSQLSESS)
*                SRCMBR(RFSWHSE)
*                COMMIT(*NONE)
*                PROCESS(*RUN)
*                OUTPUT(*ERROR)
*
update crew      set crwhse =525 where crwhse =030;
update docdoor   set ddwhse =525 where ddwhse =030;
update nexttmpl  set tnwhse =525 where tnwhse =030;
update optiond   set opdwhs =525 where opdwhs =030;
update optione   set opewhs =525 where opewhs =030;
update optionz   set opzwhs =525 where opzwhs =030;
update options   set opwhse =525 where opwhse =030;
update pirord#   set nxowhs =525 where nxowhs =030;
update pirrecid# set nriwhse=525 where nriwhse=030;
update pirrte#   set nxrwhs =525 where nxrwhs =030;
update piruser   set uswhse =525 where uswhse =030;
update piruser   set uswhse =525 where uswhse =001;
update shift     set shwhse =525 where shwhse =030;
update slotdef   set sdwhse =525 where sdwhse =030;
update taskdet   set tdwhse =525 where tdwhse =030;
update trkcomp   set tpwhse =525 where tpwhse =030;
update truckd    set tdwhse =525 where tdwhse =030;
update truckh    set thwhse =525 where thwhse =030;
update zoneh     set zhwhse =525 where zhwhse =030;
update zoned     set zdwhse =525 where zdwhse =030;
update warehs    set whcode =525 where whcode =030;
update warehsad  set whacod =525 where whacod =030;
update whdept    set wdwhse =525 where wdwhse =030;
